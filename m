+// SPDX-License-Identifier: GPL-2.0
+/*
+ * KUnit tests for curve25519.
+ * Copyright (C) 2024, LKCAMP
+ * Author: Jo=C3=A3o Pedro Le=C3=B4ncio <jpleoncio@lkcamp.dev>
+ * Author: Enzo Bertoloti <ebertoloti@lkcamp.dev>
+ */
+
+#include <kunit/test.h>
+#include <crypto/curve25519.h>
+
+/* This number comes from the original curve25519 paper
+ * (https://cr.yp.to/ecdh/curve25519-20060209.pdf) by Bernstein
+ * and as basepoint of the curve25519 function.
+ */
+#define CURVE25519_MAGIC_NUMBER 9
+
+struct curve25519_test_vector {
+=09u8 private[CURVE25519_KEY_SIZE];
+=09u8 public[CURVE25519_KEY_SIZE];
+=09u8 result[CURVE25519_KEY_SIZE];
+=09bool valid;
+};
+static const struct curve25519_test_vector curve25519_test_vectors[] =3D {
+=09{
+=09=09.private =3D { 0x77, 0x07, 0x6d, 0x0a, 0x73, 0x18, 0xa5, 0x7d,
