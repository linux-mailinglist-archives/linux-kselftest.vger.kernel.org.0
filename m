+
+static void curve25519_test(struct kunit *test)
+{
+=09bool ret;
+=09size_t i;
+=09u8 out[CURVE25519_KEY_SIZE];
+
+=09for (i =3D 0; i < ARRAY_SIZE(curve25519_test_vectors); i++) {
+=09=09memset(out, 0, CURVE25519_KEY_SIZE);
+=09=09ret =3D curve25519(out, curve25519_test_vectors[i].private,
+=09=09=09=09 curve25519_test_vectors[i].public);
+=09=09KUNIT_EXPECT_EQ(test, ret, curve25519_test_vectors[i].valid);
+=09=09KUNIT_EXPECT_EQ(test, memcmp(out, curve25519_test_vectors[i].result,
+=09=09=09   CURVE25519_KEY_SIZE), 0);
+=09}
+}
+
+static void curve25519_generate_public_test(struct kunit *test)
+{
+=09bool ret, ret2;
+=09size_t i;
+=09u8 out[CURVE25519_KEY_SIZE];
+=09u8 out2[CURVE25519_KEY_SIZE];
+=09u8 public[CURVE25519_KEY_SIZE] =3D { CURVE25519_MAGIC_NUMBER };
+
+=09for (i =3D 0; i < ARRAY_SIZE(curve25519_test_vectors); i++) {
+=09=09memset(out, 0, CURVE25519_KEY_SIZE);
+=09=09ret =3D curve25519_generate_public(out, curve25519_test_vectors[i].p=
rivate);
+=09=09ret2 =3D curve25519(out2, curve25519_test_vectors[i].private, public=
);
+=09=09KUNIT_EXPECT_EQ(test, ret, ret2);
+=09=09KUNIT_EXPECT_EQ(test, memcmp(out, out2, CURVE25519_KEY_SIZE), 0);
+=09}
+}
+
+static void curve25519_generic_test(struct kunit *test)
+{
+=09size_t i;
+=09u8 out[CURVE25519_KEY_SIZE];
+
+=09for (i =3D 0; i < ARRAY_SIZE(curve25519_test_vectors); i++) {
+=09=09memset(out, 0, CURVE25519_KEY_SIZE);
+=09=09curve25519_generic(out, curve25519_test_vectors[i].private,
+=09=09=09=09   curve25519_test_vectors[i].public);
+=09=09KUNIT_EXPECT_EQ(test, memcmp(out, curve25519_test_vectors[i].result,
+=09=09=09   CURVE25519_KEY_SIZE), 0);
+=09}
+}
+
+static struct kunit_case curve25519_test_cases[] =3D {
+=09KUNIT_CASE(curve25519_test),
+=09KUNIT_CASE(curve25519_generate_public_test),
+=09KUNIT_CASE(curve25519_generic_test),
+=09{},
+};
+
+static struct kunit_suite curve25519_test_suite =3D {
+=09.name =3D "curve25519",
+=09.test_cases =3D curve25519_test_cases,
+};
+kunit_test_suite(curve25519_test_suite);
